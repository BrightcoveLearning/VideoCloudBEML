  selected: #A2A2A2;
}
.linkText {
  active: #E8E8E8;
  disabled: #6B6B6B;
  rollover: #B2C846;
  downState: #B2C846;
}
.buttons {
  gradientTop: #7C7B7B;
  gradientBottom: #3B3A3A;
  borderGradientTop: #898989;
  borderGradientBottom: #585858;
  rolloverGradientTop: #7C7B7B;
  rolloverGradientBottom: #3B3A3A;
  downStateGradientTop: #7C7B7B;
  downStateGradientBottom: #3B3A3A;
  downStateBorder: #B0C645;
  text: #FFFFFF;
  rolloverText: #96A93B;
  disabledText: #FFFFFF;
  icons: #FFFFFF;
  rolloverIcons: #96A93B;
  disabledIcons: #7E7E7E;
}
.thumbnail {
  innerBorderGradientTop: #363636;
  innerBorderGradientBottom: #272727;
  innerBorderRolloverGradientTop: #888888;
  innerBorderRolloverGradientBottom: #4B4B4B;
  innerBorderSelectedGradientTop: #B2C846;
  innerBorderSelectedGradientBottom: #8C9E37;
  outerBorderGradientTop: #363636;
  outerBorderGradientBottom: #272727;
  outerBorderRolloverGradientTop: #888888;
  outerBorderRolloverGradientBottom: #4B4B4B;
